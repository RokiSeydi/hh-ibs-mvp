# Environment Variables for Stripe Integration
# Copy this to .env and fill in your actual values

# Stripe Keys (Get these from your Stripe Dashboard)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Database URL (if using a real database)
DATABASE_URL=your_database_url_here

# Testing Mode (set to true for small test amounts)
STRIPE_TEST_MODE=false

# Example values for testing:
# VITE_STRIPE_PUBLISHABLE_KEY=pk_test_51234567890abcdef...
# STRIPE_SECRET_KEY=sk_test_51234567890abcdef...
# STRIPE_WEBHOOK_SECRET=whsec_1234567890abcdef...
# STRIPE_TEST_MODE=true
